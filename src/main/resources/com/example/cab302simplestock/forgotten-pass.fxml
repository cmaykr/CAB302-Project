<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.cab302simplestock.controller.ForgottenpassController"
            prefHeight="400.0" prefWidth="600.0" stylesheets="@fogottenpassword.css">

    <center>
        <VBox>
            <HBox alignment="CENTER" spacing="10">
                <Label fx:id ="Email">Email</Label>
                <TextField fx:id="Username" promptText="MyEmail@gmail.com" maxWidth="200"/>
                <Button fx:id="submit" text="Submit" />
            </HBox>
        </VBox>
    </center>
</BorderPane>
